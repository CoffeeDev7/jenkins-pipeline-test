pipeline{
  agent any

  stages{
    stage('build'){
      steps{echo 'building...'}
    }
    stage('test'){
      steps{ echo 'testing...'}
    }
    stage('deploy'){
      steps{ echo 'deployed.'}
    }
    stage ('Now testing if jenkins' github-webhook triggers on sample push'){
      steps{ echo 'something is pushed to github....thus triggered' }
    }
  }
}
